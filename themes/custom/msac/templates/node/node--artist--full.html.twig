{% extends "node--full.html.twig" %}

{% block node_hero %}
  <div class="hero{% if not content.field_artist_cover|field_value %} hero--default{% endif %}">
    <figure class="hero__media">
      <div class="hero__media-image">
        {% if content.field_artist_cover|field_value %}
          {{ content.field_artist_cover }}
        {% else %}
          <img src="{{ assets_path ~ '/img/MSAC_default_profile_image.jpg' }}" alt="">
        {% endif %}
      </div>
      {% if content.field_artist_cover_caption|field_value %}
        <figcaption class="hero__media-caption">{{ content.field_artist_cover_caption }}</figcaption>
      {% endif %}
    </figure>
  </div>
{% endblock %}

{% block node_heading %}
	<div class="page-heading outer-pad">
		{% set thumbnail = assets_path ~ '/img/MSAC_default_profile_image_square.jpg' %}
		{% set thumbnail_alt = 'Portrait of ' ~ node.title.value %}
		{% if content.field_artist_photo|field_value %}
			{% set thumbnail = file_url(content.field_artist_photo|field_target_entity.uri.value | image_style('optimized')) %}
			{% set thumbnail_alt = content.field_artist_photo['#items'].alt %}
		{% endif %}
		<div class="silc-grid silc-grid--collapse silc-grid--justify-center">
			<div class="silc-position--relative silc-grid__col silc-grid__col--3-1024 silc-grid__col--2-1200">
				<div class="profile-photo__container">
					<div class="profile__photo">
						<img src="{{ thumbnail }}" alt="{{ thumbnail_alt }}">
					</div>
					<a href="{{ drupal_url('directory', {absolute: true}) }}"><span role="presentation">&lt;&nbsp;</span>Back to Directory</a>
				</div>
			</div>
			<div class="silc-grid__col {% if content.field_artist_awards|field_value %}silc-grid__col--9-1024 silc-grid__col--6-1200{% else %}silc-grid__col--9-1024 silc-grid__col--10-1200{% endif %}">
				<h1>{{ label }}</h1>
				{% if content.field_artist_artistic_categories[0] %}
					{% set disciplines = content.field_artist_artistic_categories|field_value %}
					<div class="profile__disciplines">{{ disciplines|sort|safe_join(", ") }}</div>
				{% endif %}
			</div>
			{% if content.field_artist_awards|field_value %}
				<div class="silc-grid__col silc-grid__col--5-1024 silc-grid__col--4-1200">
					<div class="artist-awards">
						<h2 class="artist-awards__heading">Awards Received</h2>
						<div class="artist-awards__content">
							{{ content.field_artist_awards|field_value }}
						</div>
					</div>
				</div>
			{% endif %}
		</div>
	</div>
{% endblock %}

{% block node_content %}
	<div class="page-content outer-pad">
		{{ drupal_messages() }}
		{% if content.field_artist_quote|field_value %}
			<div class="widget-spacing">
				<div class="silc-grid silc-grid--collapse silc-grid--justify-center">
					<div class="silc-grid__col silc-grid__col--10-800 silc-grid__col--8-1200">
						<blockquote class="blockquote blockquote--full-width">
							<div class="blockquote__quote">
								<p>{{ content.field_artist_quote|field_value }}</p>
							</div>
						</blockquote>
					</div>
				</div>
			</div>
		{% endif %}
		<div class="widget-spacing">
			<div class="silc-grid silc-grid--collapse silc-grid--justify-center silc-grid--justify-right-1024">
				<div class="silc-grid__col silc-grid__col--10-800 silc-grid__col--6-1024 silc-grid__col--6-1200">
					<div class="editor-widget text-content">
						{% if content.body %}
							<div class="collapsible-text text-content">
								<h2>About the Artist</h2>
								{{ content.body }}
							</div>
						{% endif %}
						{% if content.field_artist_websites|field_value %}
							<p>
								{% for key, item in content.field_artist_websites %}
								{% if key|first != '#' %}
									<a href="{{ content.field_artist_websites[key]['#url'] }}" class="button" target="_blank">
										<span class="visible-for-screen-readers">{{ node.title.value }} website </span>{{ content.field_artist_websites[key]['#title'] }}
									</a>
								{% endif %}
								{% endfor %}
							</p>
						{% endif %}
					</div>
					{% if content.field_artist_statement|field_value %}
						<div class="editor-widget text-content">
							<div class="collapsible-text text-content">
								<h2>Artist's Statement</h2>
								{{ content.field_artist_statement|field_value }}
							</div>
						</div>
					{% endif %}
				</div>
				<div class="silc-grid__col silc-grid__col--10-800 silc-grid__col--5-1024 silc-grid__col--4-1200">
					<aside class="aside">
						<div class="aside__content">
							{% if content.field_artist_resume|field_value %}
								<div class="aside__section">
									<h3 class="aside__section-heading">Resume</h3>
									<div>
										<p>
											<a href="{{ file_url (node.field_artist_resume.entity.uri.value) }}" target="_blank">Artist's Resume</a>
										</p>
									</div>
								</div>
							{% endif %}
							{% set contact_information %}
								{% if content.field_artist_phone|field_value %}
									<li>
										{{ content.field_artist_phone|field_value }}
									</li>
								{% endif %}
								{% if node.field_artist_show_email.value %}
									{% if content.field_artist_email|field_value %}
										<li>
											<a href="mailto:{{ content.field_artist_email|field_value }}">{{ content.field_artist_email|field_value }}</a>
										</li>
									{% endif %}
								{% endif %}
							{% endset %}
							{% if contact_information is not empty %}
								<div class="aside__section">
									<h3 class="aside__section-heading">Contact Information</h3>
									<div>
										<ul>
											{{ contact_information }}
										</ul>
									</div>
								</div>
							{% endif %}

							{% set contact_location %}
								{% if content.field_county|field_value %}
									<h3 class="aside__section-heading">County</h3>
								{% endif %}
								<div>
									{% if content.field_county|field_value %}
										<span>{{ content.field_county|field_value }}</span>
									{% endif %}
									{# {% if content.field_artist_address|field_value %}
										<div class="address-wrapper">
											<div class="icon-circle">
												<svg xmlns="http://www.w3.org/2000/svg" width="17" height="22" viewbox="0 0 17 22">
													<g fill="none" fill-rule="evenodd"><path d="M8.5 22C14.167 15.974 17 11.424 17 8.35 17 3.737 13.194 0 8.5 0S0 3.738 0 8.35C0 11.423 2.833 15.973 8.5 22z" fill="#FFF"/><circle fill="#000" cx="8.5" cy="8.5" r="3.5"/></g>
												</svg>
											</div>
											<div>
												{{ content.field_artist_address|field_value }}
											</div>
										</div>
									{% endif %} #}
								</div>
							{% endset %}
							{% if contact_location is not empty %}
								<div class="aside__section">
									{{ contact_location }}
								</div>
							{% endif %}
							<div class="aside__section">
								{% if content.field_artist_districts|field_value %}
									<h3 class="aside__section-heading">A&amp;E Districts</h3>
									<div>
										<ul>
											{% for key, item in content.field_artist_districts %}
											{% if key|first != '#' %}
												<li>{{ item }}</li>
											{% endif %}
											{% endfor %}
										</ul>
									</div>
								{% endif %}

								{% if content.field_artist_grants|field_value %}
									<div class="aside__section">
										<h3 class="aside__section-heading">Funding Available</h3>
										<div>
											<p>
												{% for item in content.field_artist_grants|field_value %}
													{{ item }}<br />
												{% endfor %}
											</p>
										</div>
									</div>
								{% endif %}
								{% set social_links %}
