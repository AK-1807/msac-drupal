		<div class="widget-spacing">
			<div class="silc-grid silc-grid--collapse silc-grid--justify-center">
				<div class="silc-grid__col silc-grid__col--10-800 silc-grid__col--8-1200">
					{% set featured_work %}
						{% if drupal_view_result('featured_work_images', 'embed_1') %}
							<h3 id="featured-work--image">Photos</h3>
							{{ drupal_view('featured_work_images', 'embed_1') }}
						{% endif %}
							{% if drupal_view_result('featured_work_videos', 'embed_1') %}
							<h3 id="featured-work--video">Videos</h3>
							{{ drupal_view('featured_work_videos', 'embed_1') }}
						{% endif %}
							{% if drupal_view_result('featured_work_audio', 'embed_1') %}
							<h3 id="featured-work--audio">Music/Audio</h3>
							{{ drupal_view('featured_work_audio', 'embed_1') }}
						{% endif %}
						{% if drupal_view_result('featured_work_documents', 'embed_1') %}
							<h3 id="featured-work--document">Written Works</h3>
							{{ drupal_view('featured_work_documents', 'embed_1') }}
						{% endif %}
					{% endset %}

					{% if featured_work|trim is not empty %}
						<h2>Featured Work</h2>
						{{ featured_work }}
					{% endif %}

					{% set booking_details %}
						{% if content.field_artist_bk_contact_info|field_value %}
							<div class="silc-accordion__section">
								<button id="booking-info-accordion-section-1-label" class="silc-accordion__label" aria-controls="booking-info-accordion-section-1-content">
									Booking Details and Contact Info
									<svg class="silc-accordion__label-icon" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24">
										<g fill-rule="evenodd"><path d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0zm0 2C6.477 2 2 6.477 2 12s4.477 10 10 10 10-4.477 10-10S17.523 2 12 2z"/><path d="M10.907 6v4.908H6v2.185h4.907V18h2.185v-4.907H18v-2.185h-4.908V6z"/></g>
									</svg>
								</button>
								<div class="silc-accordion__content" id="booking-info-accordion-section-1-content" aria-labelledby="booking-info-accordion-section-1-label">
									<div class="silc-accordion__content-compartment text-content">
										{{ content.field_artist_bk_contact_info|field_value }}
									</div>
								</div>
							</div>
						{% endif %}
						{% if content.field_artist_bk_materials_tech|field_value %}
							<div class="silc-accordion__section">
								<button id="booking-info-accordion-section-2-label" class="silc-accordion__label" aria-controls="booking-info-accordion-section-2-content">
									Materials &amp; Technology Needed
									<svg class="silc-accordion__label-icon" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24">
										<g fill-rule="evenodd"><path d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0zm0 2C6.477 2 2 6.477 2 12s4.477 10 10 10 10-4.477 10-10S17.523 2 12 2z"/><path d="M10.907 6v4.908H6v2.185h4.907V18h2.185v-4.907H18v-2.185h-4.908V6z"/></g>
									</svg>
								</button>
								<div class="silc-accordion__content" id="booking-info-accordion-section-2-content" aria-labelledby="booking-info-accordion-section-2-label">
									<div class="silc-accordion__content-compartment text-content">
										{{ content.field_artist_bk_materials_tech|field_value }}
									</div>
								</div>
							</div>
						{% endif %}
						{% if content.field_artist_bk_travel_req_rest|field_value %}
							<div class="silc-accordion__section">
								<button id="booking-info-accordion-section-3-label" class="silc-accordion__label" aria-controls="booking-info-accordion-section-3-content">
									Travel Requirements &amp; Restrictions
									<svg class="silc-accordion__label-icon" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24">
										<g fill-rule="evenodd"><path d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0zm0 2C6.477 2 2 6.477 2 12s4.477 10 10 10 10-4.477 10-10S17.523 2 12 2z"/><path d="M10.907 6v4.908H6v2.185h4.907V18h2.185v-4.907H18v-2.185h-4.908V6z"/></g>
									</svg>
								</button>
								<div class="silc-accordion__content" id="booking-info-accordion-section-3-content" aria-labelledby="booking-info-accordion-section-3-label">
									<div class="silc-accordion__content-compartment text-content">
										{{ content.field_artist_bk_travel_req_rest|field_value }}
									</div>
								</div>
							</div>
						{% endif %}
						{% if content.field_artist_bk_references|field_value %}
							<div class="silc-accordion__section">
								<button id="booking-info-accordion-section-4-label" class="silc-accordion__label" aria-controls="booking-info-accordion-section-4-content">
									References
									<svg class="silc-accordion__label-icon" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24">
										<g fill-rule="evenodd"><path d="M12 0c6.627 0 12 5.373 12 12s-5.373 12-12 12S0 18.627 0 12 5.373 0 12 0zm0 2C6.477 2 2 6.477 2 12s4.477 10 10 10 10-4.477 10-10S17.523 2 12 2z"/><path d="M10.907 6v4.908H6v2.185h4.907V18h2.185v-4.907H18v-2.185h-4.908V6z"/></g>
									</svg>
								</button>
								<div class="silc-accordion__content" id="booking-info-accordion-section-4-content" aria-labelledby="booking-info-accordion-section-4-label">
									<div class="silc-accordion__content-compartment text-content">
										{{ content.field_artist_bk_references|field_value }}
									</div>
								</div>
							</div>
						{% endif %}
					{% endset %}

					{% if booking_details|trim is not empty or content.field_artist_bk_price|field_value %}
						<div class="widget-spacing">
							<h2>Booking</h2>
							{% if content.field_artist_bk_price|field_value %}
								<p>Booking Price: {{ content.field_artist_bk_price|field_value }}</p>
							{% endif %}
							{% if booking_details|trim is not empty %}
								<div>
									<div class="silc-accordion" id="booking-info-accordion" data-silc-accordion-animated>
										{{ booking_details }}
									</div>
								</div>
							{% endif %}
						</div>
					{% endif %}

					{% if node.field_artist_arts_in_edu_roster.value %}
						{% set education_for %}
							{% if content.field_artist_population_types|field_value %}
								<p><strong>Audiences Types:</strong>&nbsp;{{ content.field_artist_population_types|field_value|safe_join(', ') }}</p>
							{% endif %}
						{% endset %}

						{% set education_info %}
							{% if node.field_artist_engmt_type_1.value and content.field_artist_engmt_type_1_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Lectures Offered', 'index': 1, 'content': content.field_artist_engmt_type_1_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_2.value and content.field_artist_engmt_type_2_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Online Programs Offered', 'index': 2, 'content': content.field_artist_engmt_type_2_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_3.value and content.field_artist_engmt_type_3_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Out of School Time Offered', 'index': 3, 'content': content.field_artist_engmt_type_3_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_4.value and content.field_artist_engmt_type_4_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Residencies Offered', 'index': 4, 'content': content.field_artist_engmt_type_4_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_5.value and content.field_artist_engmt_type_5_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Visiting Performances Offered', 'index': 5, 'content': content.field_artist_engmt_type_5_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_6.value and content.field_artist_engmt_type_6_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Workshops Offered', 'index': 6, 'content': content.field_artist_engmt_type_6_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_7.value and content.field_artist_engmt_type_7_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Professional Development Offered', 'index': 7, 'content': content.field_artist_engmt_type_7_text|field_value} %}
							{% endif %}
							{% if node.field_artist_engmt_type_8.value and content.field_artist_engmt_type_8_text|field_value %}
								{% include directory ~ '/templates/helpers/accordion-section.html.twig' with {'title': 'Field Trips Offered', 'index': 8, 'content': content.field_artist_engmt_type_8_text|field_value} %}
							{% endif %}
						{% endset %}

						{% if education_for is not empty or education_info is not empty %}
							<div class="widget-spacing">
								<h2>Arts in Education</h2>
								{% if education_for %}
									{{ education_for }}
								{% endif %}
								{% if education_info %}
									<div>
										<div class="silc-accordion" id="education-info-accordion" data-silc-accordion-animated>
											{{ education_info }}
										</div>
									</div>
								{% endif %}
							</div>
						{% endif %}
					{% endif %}
				</div>
			</div>
		</div>
	</div>
{% endblock %}
